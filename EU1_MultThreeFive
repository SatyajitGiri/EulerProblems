#Written in C
#Euler Problem 1 (Multiples of 3 and 5)

#include<stdio.h>

main(){
       int pt, pf, pfn, sum=0;
       
       for(pt=1;3*pt<1000;pt++)
                                sum=sum+(3*pt);
                                
       for(pf=1;5*pf<1000;pf++)
                                sum=sum+(5*pf);
       
       for(pfn=1;15*pfn<1000;pfn++)
                                sum=sum-(15*pfn);
       
                       
       printf("%d",sum);
       getch();
       }
                                
                                

